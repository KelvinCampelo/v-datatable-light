{
  "name": "v-datatable",
  "version": "0.1.0",
  "main": "dist/v-datatable.umd.min.js",
  "private": false,
  "keywords": [
    "vue",
    "component",
    "datatable",
    "lightweight",
    "no-dependencies"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./example/main.js --open",
    "build": "vue-cli-service build --name v-datatable --entry ./src/index.js --target lib",
    "lint": "vue-cli-service lint",
    "doc:build": "npx vuedoc.md --section API --output ./README.md ./src/index.js",
    "prepublishOnly": "npm run lint && npm run build && npm run styleguide:build && npm run doc:build",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vuedoc/md": "^1.5.0",
    "babel-eslint": "^10.0.1",
    "component-fixture": "^0.4.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "lodash.orderby": "^4.6.0",
    "vue-cli-plugin-component": "^1.10.5",
    "vue-simple-spinner": "^1.2.8",
    "vue-styleguidist": "^1.7.13",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "peerDependencies": {
    "vue": "^2.6.6"
  }
}
